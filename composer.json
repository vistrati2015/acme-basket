{
    "name": "victor/acme",
    "description": "Acme application",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\Acme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Acme\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Victor Istrati",
            "email": "viktor.istrati@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.75"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "phpstan": "./vendor/bin/phpstan analyse",
        "fix": "./vendor/bin/php-cs-fixer fix src"
    }
}
